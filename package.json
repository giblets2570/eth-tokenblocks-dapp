{
  "name": "now-ui-dashboard-pro-react",
  "version": "1.2.0",
  "private": true,
  "dependencies": {
    "@types/googlemaps": "3.30.11",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.4.11",
    "ajv": "5.0.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "4.1.3",
    "chart.js": "2.7.2",
    "ethereumjs-util": "^5.2.0",
    "moment": "2.22.2",
    "node-sass-chokidar": "1.3.3",
    "nouislider": "11.1.0",
    "npm-run-all": "4.1.3",
    "openzeppelin-solidity": "^1.12.0",
    "perfect-scrollbar": "1.4.0",
    "react": "16.4.2",
    "react-app-env": "^1.2.2",
    "react-big-calendar": "0.19.2",
    "react-bootstrap-sweetalert": "4.4.1",
    "react-bootstrap-switch": "15.5.3",
    "react-bootstrap-wizard": "0.0.5",
    "react-chartjs-2": "2.7.4",
    "react-csv-reader": "^0.3.2",
    "react-datepicker": "^1.6.0",
    "react-datetime": "2.15.0",
    "react-dom": "16.4.2",
    "react-google-maps": "9.4.5",
    "react-jvectormap": "0.0.3",
    "react-notification-alert": "0.0.8",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "react-select": "2.0.0",
    "react-table": "6.8.6",
    "react-tagsinput": "3.19.0",
    "reactstrap": "6.4.0",
    "socket.io-client": "^2.1.1",
    "tiny-promisify": "^1.0.0",
    "truffle-contract": "^3.0.6",
    "truffle-hdwallet-provider": "0.0.6",
    "web3": "^0.20.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-app-env start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-app-env build && rimraf dist && mv build dist",
    "test": "react-app-env test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "build:contracts": "rimraf build/ && truffle compile && truffle migrate && npm run contracts:sync-src",
    "build:contracts:kovan": "rimraf build/ && truffle compile && truffle migrate --network=kovan && npm run contracts:sync-src:kovan",
    "contracts:sync-src": "rimraf src/contracts/local && cp -R build/contracts/ src/contracts/local",
    "contracts:sync-src:kovan": "rimraf src/contracts/kovan && cp -R build/contracts/ src/contracts/kovan",
    "seed": "export $(cat .env) && node seed.js",
    "seed:prod": "export $(cat .env.prod) && node seed.js",
    "deploy": "aws s3 sync dist/ s3://tokenblocks",
    "deploy:contracts": "aws s3 sync src/contracts/kovan/ s3://tokenblocks-contracts",
    "sync:contracts": "aws s3 sync s3://tokenblocks-contracts src/contracts/kovan/",
    "sync:bundles": "aws s3 sync bundles/ s3://tokenblocks-files/bundles",
    "transfer": "export $(cat .env) && node transfer.js",
    "update:network:port": "node update-network.js",
    "network": "npm run update:network:port; export $(cat .env); func() { ganache-cli -m=\"village rely embrace donkey video demand crunch train throw trouble resource myth\" -e 10000 -a 100 --networkId=$NETWORK; }; func"
  },
  "optionalDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "^1.13.7"
  },
  "devDependencies": {
    "mysql": "^2.16.0",
    "request-promise": "^4.2.2"
  }
}
